    "compression": "^1.7.2",
    "config": "^1.30.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "express-authz": "^1.0.1",
    "express-session": "^1.15.6",
    "express-validation": "^1.0.2",
    "fb": "^2.0.0",
    "google-auth-library": "^1.6.1",
    "helmet": "^3.12.1",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "joi": "^13.4.0",
